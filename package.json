{
  "name": "input-plugin-quote",
  "description": "Simple input plugin for quote type",
  "main": "index.js",
  "scripts": {
    "test": "npm run nyc",
    "pretest": "npm run lint",
    "ava": "ava | tap-diff",
    "ava:watch": "ava --watch | tap-diff",
    "nyc": "nyc --all npm run ava",
    "lint": "eslint index.js lib && eslint -c ./.eslintrc-ava.yml tests",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/punchcard-cms/input-plugin-quote.git"
  },
  "contributors": [
    "Scott Nath <snath@us.ibm.com>"
  ],
  "license": "Apache-2.0",
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.16.0",
    "coveralls": "^2.11.9",
    "eslint": "^3.5.0",
    "eslint-config-punchcard": "^1.1.0",
    "ghooks": "^1.3.2",
    "lodash": "^4.15.0",
    "nyc": "^6.6.1",
    "punchcard-commit-msg": "^1.0.0",
    "punchcard-semantic-release": "^2.0.1",
    "punchcard-shared-tests": "^1.0.2",
    "semantic-release": "^4.3.5",
    "tap-diff": "^0.1.1"
  },
  "engines": {
    "node": "5.11",
    "npm": "^3.6"
  },
  "keywords": [
    "input-plugin",
    "punchcard-cms"
  ],
  "nyc": {
    "exclude": [
      "tests/**/*"
    ]
  },
  "ava": {
    "files": [
      "tests/*.js"
    ],
    "failFast": true,
    "tap": true
  }
}
